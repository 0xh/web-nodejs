/* open-sans-300 - latin */
@font-face {
	font-family: 'Open Sans';
	font-style: normal;
	font-weight: 300;
	src: url($font-path + 'open-sans-v13-latin-300.eot');
	src: local('Open Sans Light'), local('OpenSans-Light'),
		url($font-path + 'open-sans-v13-latin-300.eot?#iefix') format('embedded-opentype'),
		url($font-path + 'open-sans-v13-latin-300.woff2') format('woff2'),
		url($font-path + 'open-sans-v13-latin-300.woff') format('woff'),
		url($font-path + 'open-sans-v13-latin-300.ttf') format('truetype'),
		url($font-path + 'open-sans-v13-latin-300.svg#OpenSans') format('svg');
}

